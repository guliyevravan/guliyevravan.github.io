<script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
<script src="/assets/js/fslightbox.js"></script>
<script>

  
  const breakpointNames = ['sm', 'md', 'lg', 'xl', '2xl'];
  const breakpoints = {};

  for (const name of breakpointNames) {
    const remValue = parseFloat(getComputedStyle(document.documentElement).getPropertyValue(`--breakpoint-${name}`));
    breakpoints[name] = remValue * 16;
  }
  
  const swiper = new Swiper('.swiper', {
    // Optional parameters
    direction: 'horizontal',
    loop: true,
    autoplay: {
      delay: 3000,
    },

    breakpoints: {
      [breakpoints.sm]: { slidesPerView: 2, spaceBetween: 10 },
      [breakpoints.md]: { slidesPerView: 3, spaceBetween: 10 },
      [breakpoints.lg]: { slidesPerView: 4, spaceBetween: 10 },
      [breakpoints.xl]: { slidesPerView: 5, spaceBetween: 10 },
      [breakpoints['2xl']]: { slidesPerView: 6, spaceBetween: 10 }
    },

    // If we need pagination
    pagination: {
      el: '.swiper-pagination',
      clickable: true,
    },

    // Navigation arrows
    navigation: { 
      nextEl: '.swiper-custom-next',
      prevEl: '.swiper-custom-prev',
    },

    // And if we need scrollbar
    scrollbar: {
      el: '.swiper-scrollbar',
    },
  });

</script>
